                    </div>
                    <div class="home-whats-new">
                        <span class="home-text05">What&apos;s new</span>
                    </div>
                </div>
                <div class="home-middle-boxes">
                    <div class="home-middle-boxes-top">
                        <div class="home-on-this-day">
                            <span class="home-text06">
                                <span>On this day</span>
                                <br />
                            </span>
                        </div>
                        <div class="home-yesterday">
                            <span class="home-text09">Yesterday</span>
                        </div>
                        <div class="home-here-and-now">
                            <span class="home-text10">2 days ago</span>
                        </div>
                    </div>
                    <div class="home-middle-boxes-bottom">
                        <div class="home-on-this-day1">
                            <span class="home-text11">
                                <span>Born this day</span>
                                <br />
                            </span>
                        </div>
                        <div class="home-yesterday1">
                            <span class="home-text14">Born Yesterday</span>
                        </div>
                        <div class="home-here-and-now1">
                            <span class="home-text15">Born 2 days ago</span>
                        </div>
                    </div>
                </div>
                <div class="home-side-bar-right">
                    <div class="home-here-and-now2">
                        <span class="home-text16">Here and now</span>
                    </div>
                    <div class="home-then-and-now">
                        <span class="home-text17">
                            <span>Then and now</span>
                            <br />
                        </span>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

